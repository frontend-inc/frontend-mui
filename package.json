        "babel-preset-stage-2": "^6.24.1",
        "cookies-next": "^2.1.1",
        "gh-pages": "^2.2.0",
        "next": "^14.0.4",
        "next-images": "^1.8.5",
        "react": "^17.0.1 || ^18.0.0",
        "react-dom": "^17.0.2 || ^18.0.0",
        "react-scripts": "^4.0.3",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "@apollo/client": "^3.8.9",
