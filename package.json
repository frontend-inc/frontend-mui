{
    "name": "frontend-mui",
    "version": "0.0.23",
    "license": "MIT",
    "description": "A MUI library designed for Frontend.co",
    "homepage": "https://github.com/frontend-inc/frontend-mui",
    "repository": {
        "type": "git",
        "url": "https://github.com/frontend-inc/frontend-mui"
    },
    "keywords": [
        "react",
        "mui"
    ],
    "main": "dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./api": "./dist/api/index.js",
        "./api/rest": "./dist/api/rest/index.js",
        "./assets": "./dist/assets",
        "./client": "./dist/client/index.js",
        "./components": "./dist/components/index.js",
        "./components/addons": "./dist/components/addons/index.js",
        "./components/ui": "./dist/components/ui/index.js",
        "./components/shopify": "./dist/components/shopify/index.js",
        "./constants": "./dist/constants/index.js",
        "./context": "./dist/context/index.js",
        "./context/core": "./dist/context/core/index.js",
        "./helpers": "./dist/helpers/index.js",
        "./hooks": "./dist/hooks/index.js",
        "./hooks/api": "./dist/hooks/api/index.js",
        "./hooks/addons": "./dist/hooks/addons/index.js",
        "./hooks/common": "./dist/hooks/common/index.js",
        "./theme": "./dist/theme/index.js"
    },
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "babel": {
        "presets": [
            "react",
            "env",
            "stage-2"
        ]
    },
    "scripts": {
        "build": "npx rimraf dist && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --outDir dist",
        "bump": "./bump-version.sh",
        "deploy": "git add . && git commit -m 'deploy' && git push",
        "push": "yarn bump && yarn build && yarn deploy"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.8.9",
